    FarmTab:Toggle{
        Name = "Auto Push Up",
        StartingState = false,
        Description = "automaticly gain hp for u and u can still walk don't worry!",
        Callback = function(v)
            getgenv().autopushup = v
            while true do 
                if not getgenv().autopushup then return end
                local Event = game:GetService("ReplicatedStorage").Remotes.PushUp
                Event:FireServer()
                wait()
            end
        end
    }

